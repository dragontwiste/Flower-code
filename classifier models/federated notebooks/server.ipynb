    "import os/n",
    "import flwr as fl/n",
    "import torch/n",
    "import torch.nn as nn/n",
    "from torchvision import models, transforms/n",
    "from torch.utils.data import DataLoader, Dataset/n",
    "from PIL import Image/n",
    "import pandas as pd/n",
    "import pydicom/n",
    "import numpy as np/n",
    "from collections import OrderedDict/n",
    "from sklearn.metrics import accuracy_score, recall_score/n",
    "from flwr.server.strategy import FedAvg, DPFedAvgFixed/n",
    "from flwr.common import parameters_to_ndarrays/n",
    "from typing import List, Tuple, Dict/n",
    "/n",
    "# ‚úÖ Config/n",
    "XLS_PATH = r/"E://PFE//Flower code//data original//INbreast Release 1.0//INbreast.xls/"/n",
    "ALLDICOMs = r/"E://PFE//Flower code//data original//INbreast Release 1.0//AllDICOMs/"/n",
    "SAVE_PATH = r/"E://PFE//Flower code//classifier models/"/n",
    "/n",
    "# ‚úÖ Label mapping/n",
    "def map_label(bi_rads):/n",
    "    return 0 if str(bi_rads).strip() == /"2/" else 1/n",
    "/n",
    "# ‚úÖ Dataset for server eval/n",
    "class InbreastDataset(Dataset):/n",
    "    def __init__(self, df, dicom_dir, transform):/n",
    "        self.df = df/n",
    "        self.dicom_dir = dicom_dir/n",
    "        self.transform = transform/n",
    "/n",
    "    def __len__(self):/n",
    "        return len(self.df)/n",
    "/n",
    "    def __getitem__(self, idx):/n",
    "        row = self.df.iloc[idx]/n",
    "        file_name = str(int(row['File Name']))/n",
    "        label = row['label']/n",
    "        dicom_file = next((f for f in os.listdir(self.dicom_dir) if f.startswith(file_name)), None)/n",
    "        dcm_path = os.path.join(self.dicom_dir, dicom_file)/n",
    "        image = pydicom.dcmread(dcm_path).pixel_array.astype(np.float32)/n",
    "        image = (image - image.min()) / (image.max() - image.min())/n",
    "        image = (image * 255).astype(np.uint8)/n",
    "        image = Image.fromarray(image).convert(/"RGB/")/n",
    "        image = self.transform(image)/n",
    "        return image, label/n",
    "/n",
    "# ‚úÖ Transform and eval set/n",
    "val_transform = transforms.Compose([/n",
    "    transforms.Resize((640, 640)),/n",
    "    transforms.ToTensor(),/n",
    "    transforms.Normalize([0.5]*3, [0.5]*3),/n",
    "])/n",
    "/n",
    "df = pd.read_excel(XLS_PATH)/n",
    "df.columns = df.columns.str.strip()/n",
    "df = df.dropna(subset=[/"File Name/"])/n",
    "df = df[df[/"Mass/"] == /"X/"]/n",
    "df = df[df[/"Bi-Rads/"].astype(str).isin([/"2/", /"3/", /"4/", /"5/", /"6/", /"1/"])].copy()/n",
    "df[/"label/"] = df[/"Bi-Rads/"].apply(map_label)/n",
    "val_df = df.sample(n=5, random_state=42).reset_index(drop=True)/n",
    "val_loader = DataLoader(InbreastDataset(val_df, ALLDICOMs, val_transform), batch_size=8)/n",
    "/n",
    "# ‚úÖ Model/n",
    "def create_model():/n",
    "    model = models.resnet50(pretrained=True)/n",
    "    model.fc = nn.Linear(model.fc.in_features, 2)/n",
    "    return model/n",
    "/n",
    "# ‚úÖ Evaluation/n",
    "def evaluate_model(model, val_loader):/n",
    "    model.eval()/n",
    "    y_true, y_pred = [], []/n",
    "    with torch.no_grad():/n",
    "        for images, labels in val_loader:/n",
    "            images, labels = images.to(/"cuda/"), labels.to(/"cuda/")/n",
    "            preds = model(images)/n",
    "            y_pred += preds.argmax(1).cpu().tolist()/n",
    "            y_true += labels.cpu().tolist()/n",
    "    acc = accuracy_score(y_true, y_pred)/n",
    "    rec = recall_score(y_true, y_pred)/n",
    "    return acc, rec/n",
    "/n",
    "# ‚úÖ Custom strategy/n",
    "class EvalAndSaveFedAvg(FedAvg):/n",
    "    def aggregate_fit(/n",
    "        self,/n",
    "        rnd: int,/n",
    "        results: List[Tuple[fl.server.client_proxy.ClientProxy, fl.common.FitRes]],/n",
    "        failures: List[BaseException],/n",
    "    ) -> Tuple[fl.common.Parameters, Dict[str, float]]:/n",
    "/n",
    "        aggregated_weights, metrics = super().aggregate_fit(rnd, results, failures)/n",
    "/n",
    "        if aggregated_weights is not None and rnd > 0:/n",
    "            weights_list = parameters_to_ndarrays(aggregated_weights)/n",
    "            model = create_model().to(/"cuda/")/n",
    "            state_dict = OrderedDict({/n",
    "                k: torch.tensor(v).to(/"cuda/") for k, v in zip(model.state_dict().keys(), weights_list)/n",
    "            })/n",
    "            model.load_state_dict(state_dict, strict=True)/n",
    "/n",
    "            # ‚úÖ Save model/n",
    "            model_path = os.path.join(SAVE_PATH, f/"global_model_round_{rnd}.pt/")/n",
    "            torch.save(model.state_dict(), model_path)/n",
    "            print(f/"üíæ Saved global model at: {model_path}/")/n",
    "/n",
    "            # ‚úÖ Evaluate/n",
    "            acc, rec = evaluate_model(model, val_loader)/n",
    "            print(f/"//nüåç [Round {rnd}] Global Accuracy: {acc:.4f}, Recall: {rec:.4f}/")/n",
    "/n",
    "            return aggregated_weights, {/"accuracy/": acc, /"recall/": rec}/n",
    "/n",
    "        return aggregated_weights, metrics or {}/n",
    "/n",
    "# ‚úÖ Strategy/n",
    "base_strategy = EvalAndSaveFedAvg(/n",
    "    fraction_fit=1.0,/n",
    "    min_fit_clients=2,/n",
    "    min_available_clients=2,/n",
    ")/n",
    "/n",
    "strategy = DPFedAvgFixed(/n",
    "    strategy=base_strategy,/n",
    "    noise_multiplier=0.5,/n",
    "    clip_norm=1.0,/n",
    "    num_sampled_clients=2,/n",
    ")/n",
    "/n",
    "# ‚úÖ Start server/n",
    "fl.server.start_server(/n",
    "    server_address=/"localhost:9675/",/n",
    "    config=fl.server.ServerConfig(num_rounds=10),/n",
    "    strategy=strategy,/n",
    ")/n"
   ]
  }
 ],
